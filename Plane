from tkinter import *
def jmak():
    gey=int(ent1.get())
    tey=int(ent.get())
    hey=gey+tey
    lbl.configure(text=hey)
window = Tk()
window.title("Wellllllll")
window.geometry("450x450")
lbl=Label(window, text="pypok")
lbl.pack()
bnt=Button(window, text='=',command=jmak)
bnt.pack()
ent = Entry(window,width=10)
ent.focus()
ent.pack()
ent1 = Entry(window,width=10)
ent1.focus()
ent1.pack()
window.mainloop()
